{
  "name": "proshop",
  "version": "1.0.0",
  "description": "eCommerce app built with the MERN stack",
  "homepage": "https://github.com/Apatidar0901/proshop#readme",
  "bugs": {
    "url": "https://github.com/Apatidar0901/proshop/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apatidar0901/proshop.git"
  },
  "license": "MIT",
  "author": "Abhishek Patidar",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d",
    "build": "npm install --legacy-peer-deps && npm install --legacy-peer-deps --prefix frontend && npm run build --prefix frontend"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "express-list-endpoints": "^7.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.4",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.0",
    "nodemon": "^3.1.10"
  }
}
